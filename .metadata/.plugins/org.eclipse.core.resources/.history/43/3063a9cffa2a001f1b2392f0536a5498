package com.rts.klef.springboot.model;

@Entity
@Table(name = "ration_table")
public class Ration {
	  @Id
	  @GeneratedValue(strategy = GenerationType.IDENTITY)
	  private int id;
	  @Column(nullable = false,length = 100)
	  private String name;
	  @Column(nullable = false,length = 1000)
	  private double quantity;
	  @Column(nullable = false,length = 1000 )
	

	

}
